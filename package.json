  "repository": {
    "url": "https://github.com/Gnuxie/matrix-protection-suite-for-matrix-bot-sdk.git",
    "type": "git"
  },
  "scripts": {
    "start": "yarn tsc --project tsconfig.json --watch",
    "build": "rm -rf dist && yarn tsc --project tsconfig.json && yarn lint && yarn make:docs",
    "lint": "yarn eslint src/**/*.ts  && yarn prettier src --check",
    "make:docs": "typedoc",
    "test": "jest --coverage",
    "test:ci": "cross-env CI=1 jest",
    "prepare": "husky install && yarn build"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/js": "^9.6.0",
    "@sinclair/typebox": "0.34.13",
    "@the-draupnir-project/matrix-basic-types": "1.4.0",
    "@types/crypto-js": "^4.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/glob-to-regexp": "^0.4.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.6.0",
    "husky": "^9.0.0",
    "jest": "^30.0.0",
    "lint-staged": "^16.0.0",
    "matrix-bot-sdk": ">=0.6.4",
    "matrix-protection-suite": "npm:@gnuxie/matrix-protection-suite@3.8.0",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typedoc": "^0.26.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "./node_modules/.bin/prettier --write",
      "./node_modules/.bin/eslint"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@gnuxie/typescript-result": "^1.0.0",
    "await-lock": "^2.2.2"
  },
  "peerDependencies": {
    "@sinclair/typebox": "0.34.13",
    "@the-draupnir-project/matrix-basic-types": "1.3.0",
